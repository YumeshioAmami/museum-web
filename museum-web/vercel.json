{
    "version": 2,
    "builds": [
        {
            "src": "**/*",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/$1"
        }
    ],
    "functions": {
        "assets/videos/**": {
            "maxDuration": 30
        }
    },
    "headers": [
        {
            "source": "/assets/videos/(.*)",
            "headers": [
                {
                    "key": "Content-Type",
                    "value": "video/mp4"
                },
                {
                    "key": "Cache-Control",
                    "value": "public, max-age=31536000"
                }
            ]
        }
    ]
}
